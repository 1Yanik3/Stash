    "hash-wasm": "^4.9.0",
    "mime-types": "^2.1.35",
    "node-html-parser": "^6.1.13",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "prettier-plugin-svelte": "^3.2.3",
    "prisma": "^5.17.0",
