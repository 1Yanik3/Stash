{
  "name": "stash",
  "version": "0.0.1",
  "scripts": {
    "dev": "source ../.env && vite dev",
    "build": "svelte-kit sync && vite build",
    "start": "vite",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "pnpm run format:prettier && pnpm run format:stylelint",
    "format:prettier": "npx prettier . --write",
    "format:stylelint": "npx stylelint --fix **/*.{css,scss,svelte}"
  },
  "devDependencies": {
    "@egjs/grid": "^1.16.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@mdi/js": "^7.4.47",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "@types/fuzzy-search": "^2.1.5",
    "eslint-plugin-svelte": "2.46.1",
    "hash-wasm": "^4.12.0",
    "mime-types": "^2.1.35",
    "node-html-parser": "^6.1.13",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "prettier-plugin-svelte": "^3.3.2",
    "prisma": "^6.0.1",
    "sass": "^1.81.1",
    "stylelint": "^16.11.0",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-scss": "^6.10.0",
    "svelte": "5.4.0",
    "svelte-check": "^4.1.0",
    "svelte-loading-spinners": "^0.3.6",
    "svelte-preprocess": "^6.0.3",
    "ts-exif-parser": "^0.2.2",
    "typescript": "^5.7.2"
  },
  "type": "module",
  "dependencies": {
    "@mistralai/mistralai": "^1.3.4",
    "@prisma/client": "^6.0.1",
    "@sentry/sveltekit": "^8",
    "cookie": "^1.0.2",
    "fuse.js": "^7.0.0",
    "fuzzy-search": "^3.2.1",
    "mdi-svelte": "^1.1.2",
    "pretty-bytes": "^6.1.1",
    "select-files": "^1.0.1",
    "svelte-dropzone": "^1.0.6",
    "svelte-local-storage-store": "^0.6.4",
    "svelte-markdown": "^0.4.1",
    "vite": "^6.0.2"
  }
}
